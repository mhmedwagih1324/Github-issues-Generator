{
    "files.eol": "\n",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.pylance": "explicit",
            "source.organizeImports": "explicit",
        },
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.indentSize": "tabSize",
        "editor.tabSize": 4,
    }
}